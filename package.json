{
  "name": "material-ui-playground",
  "version": "1.0.0",
  "description": "Material-UI Playground",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build:clean": "rimraf ./dist",
    "build:dev": "npm-run-all -s build:clean webpack:dev",
    "build:prod": "npm-run-all -s build:clean webpack:prod",
    "test": "cross-env NODE_ENV=test jest",
    "test:updateSnapshot": "cross-env NODE_ENV=test jest --updateSnapshot",
    "test:coverage": "cross-env NODE_ENV=test jest --collect-coverage",
    "start:dev-server": "cross-env NODE_ENV=development webpack serve --env development",
    "format": "eslint --fix ./src && prettier --write ./src",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "webpack:dev": "cross-env NODE_ENV=development webpack --env development",
    "webpack:prod": "cross-env NODE_ENV=production webpack --env production",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "author": "tt4g",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-transform-react-constant-elements": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/csp-html-webpack-plugin": "^3.0.1",
    "@types/html-webpack-plugin": "^3.2.5",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/terser-webpack-plugin": "^5.0.3",
    "@types/webpack-dev-server": "^3.11.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "compression-webpack-plugin": "^7.1.2",
    "cross-env": "^7.0.3",
    "csp-html-webpack-plugin": "^5.1.0",
    "css-loader": "^5.2.0",
    "cssnano": "^4.1.11",
    "escape-string-regexp": "^4.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.9",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.1",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "resolve-url-loader": "^3.1.2",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.1.1",
    "typescript": "4.2.3",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@material-ui/core": "^4.11.3",
    "fontsource-roboto": "^3.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-router-dom": "^5.2.0"
  }
}
